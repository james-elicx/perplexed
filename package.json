{
  "name": "perplexed",
  "version": "3.0.0",
  "description": "Plex API Client",
  "main": "dist/index.js",
  "author": "George Czabania <george@czabania.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stayradiated/perplexed.git"
  },
  "bugs": {
    "url": "https://github.com/stayradiated/perplexed/issues"
  },
  "homepage": "https://github.com/stayradiated/perplexed#readme",
  "keywords": [
    "plex",
    "api",
    "client"
  ],
  "scripts": {
    "build": "pkg-build",
    "prepublish": "pkg-build",
    "test": "pkg-test",
    "coverage": "pkg-coverage",
    "lint": "pkg-lint"
  },
  "dependencies": {
    "assert": "^1.4.1",
    "deep-diff": "^0.3.8",
    "difflet": "^1.0.1",
    "es6-promisify": "^5.0.0",
    "isomorphic-base64": "^1.0.2",
    "isomorphic-fetch": "^2.2.1",
    "normalizr": "^3.2.3",
    "qs": "^6.5.1",
    "uuid": "^3.1.0",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "@mishguru/package": "^1.5.3",
    "nock": "^9.0.22"
  },
  "babel": {
    "presets": [
      "flow",
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      [
        "transform-object-rest-spread",
        {
          "useBuiltIns": true
        }
      ]
    ]
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
