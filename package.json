{
  "name": "perplexed",
  "version": "4.1.0",
  "description": "Plex API Client",
  "main": "dist/index.js",
  "author": "George Czabania <george@czabania.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stayradiated/perplexed.git"
  },
  "bugs": {
    "url": "https://github.com/stayradiated/perplexed/issues"
  },
  "homepage": "https://github.com/stayradiated/perplexed#readme",
  "keywords": [
    "plex",
    "api",
    "client"
  ],
  "scripts": {
    "build": "rollup -c",
    "prepublish": "rollup -c",
    "test": "pkg-test",
    "coverage": "pkg-coverage",
    "lint": "pkg-lint"
  },
  "dependencies": {
    "assert": "^1.4.1",
    "babel-polyfill": "^6.26.0",
    "deep-diff": "^0.3.8",
    "difflet": "^1.0.1",
    "es6-promisify": "^5.0.0",
    "isomorphic-base64": "^1.0.2",
    "isomorphic-fetch": "^2.2.1",
    "normalizr": "^3.2.3",
    "qs": "^6.5.1",
    "uuid": "^3.1.0",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "nock": "^9.0.22",
    "@mishguru/package": "^1.5.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "rollup-plugin-json": "^2.3.0",
    "rollup": "^0.53.3",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-local-resolve": "^1.0.7",
    "eslint-config-mishguru": "^1.10.0"
  },
  "babel": {
    "presets": [
      "flow",
      [
        "env",
        {
          "forceAllTransforms": true
        }
      ]
    ],
    "plugins": [
      [
        "transform-object-rest-spread",
        {
          "useBuiltIns": true
        }
      ]
    ]
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
